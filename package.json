{
  "name": "darmasr",
  "version": "1.0.0",
  "description": "Compound Elections Management System",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "seed:buildings": "node server/scripts/seedBuildings.js",
    "check-env": "node server/scripts/checkEnv.js",
    "verify-pm2": "node server/scripts/verifyPM2.js",
    "check-status": "./check-status.sh",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:logs:api": "pm2 logs darmasr-api",
    "pm2:logs:client": "pm2 logs darmasr-client",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:start:prod": "npm run build && pm2 start ecosystem.config.prod.js --env production",
    "pm2:start:dev": "pm2 start ecosystem.config.js",
    "start": "./start.sh",
    "fix-client": "./fix-client.sh"
  },
  "keywords": [
    "elections",
    "compound",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.955.0",
    "@aws-sdk/s3-request-presigner": "^3.955.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}